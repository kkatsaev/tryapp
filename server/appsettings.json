{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting": "Information"
    },
    "Console": {
      "FormatterName": "console",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "[HH:mm:ss.ffffff] "
      }
    }
  },
  "AllowedHosts": "*"
}
